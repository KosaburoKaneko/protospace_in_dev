- if !current_user?(prototype.user)
  %span.like
    - if prototype.good?(current_user)
      = form_for(prototype.likes.find(user_id: current_user.id), method: :delete, remote: true) do |f|
        = button_tag(class: "btn btn-default btn-xs") do
          = content_tag :span, "#", class: "glyphicon glyphicon-heart"
    - else
      = form_for(prototype.likes.build, remote: true) do |f|
        %div= hidden_field_tag :prototype_id, prototype.id
        = button_tag(class: "btn btn-default btn-xs") do
          = content_tag :span, "#", class: "glyphicon glyphicon-heart-empty"
